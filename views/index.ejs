<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="/css/index.css">
    <meta charset="utf-8">
    <title>Test</title>
  </head>
  <body>
      <%- include('./partials/sidebar.ejs')%>
    <div class="maincontent">
    <h1>Song playing?</h1>
    <audio controls id="audioelement" src="<%="/audio/" + songs[0].songID %>" type="audio/mp3">please</audio>
    <h1>List of songs on db: </h1>
    <table id="songlist">
        <thead>
            <tr>
                <th>Title</th>
                <th>Artist</th>
                <th>Length</th>
            </tr>
        </thead>
        <tbody>
            <%songs.forEach(function(song) {%>
                <tr><th id="<%=song.songID%>"><%=song.SongName%></th><th id="<%=song.songID%>"><%=song.Artist%></th><th id="<%=song.songID%>"><%=song.Length%></th></tr>
            <%})%>
        </tbody>
    </table>
</div>
    <div class="z-depth-5 control">
        <p>Control div</p>
        <div class="progress">
            <div class="determinate" style="width: 70%"></div>
        </div>
</div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="js/play.js"
    <script>
    document.addEventListener('DOMContentLoaded', function() {
        var elems = document.querySelectorAll('.sidenav');
        var instances = M.Sidenav.init(elems);
    });
    </script>
  </body>
</html>
